<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>

		<Console name="STDOUT" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss} %-5p (%F:%L) -> %m%n"/>
		</Console>
		<Console name="STDOUT_ERRORS" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss} %-5p ooooooooo (%F:%L) %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Logger name="mp" level="info" additivity="false">
			<AppenderRef ref="STDOUT"/>
		</Logger>
		<Logger name="com.j256" level="info" additivity="false">
			<AppenderRef ref="STDOUT"/>
		</Logger>
		<Root level="info">
		</Root>
	</Loggers>
</Configuration>